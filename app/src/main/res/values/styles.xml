<resources xmlns:tools="http://schemas.android.com/tools">

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/Gray800</item>
        <item name="colorPrimaryDark">@color/material_grey_900</item>
        <item name="colorAccent">@color/material_grey_850</item>
        <item name="android:textColorPrimary">@color/white</item>
        <item name="android:textColorSecondary">@color/Gray50</item>

    </style>

    <style name="CustomTheme1" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/Gray800</item>
        <item name="colorPrimaryDark">@color/material_grey_900</item>
        <item name="colorAccent">@color/material_grey_850</item>
        <item name="android:textColorPrimary">@color/white</item>
        <item name="android:textColorSecondary">@color/Gray50</item>
    </style>

    <style name="CustomTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>


    <style name="AppTheme.ActionBar.Transparent" parent="AppTheme">

        <item name="android:windowContentOverlay">@null</item>
        <item name="windowActionModeOverlay">true</item>
        <item name="colorPrimary">@color/transparent_actionbar_color</item>
        <item name="colorPrimaryDark">@color/LimeA400</item>
        <!--  <item name="android:windowContentOverlay">@null</item>
         <item name="windowActionBarOverlay">true</item>-->
     </style>



 </resources>
